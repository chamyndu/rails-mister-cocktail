<h2>Add doses</h2>
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-6 col-sm-offset-3"></div>
    <%= simple_form_for [@cocktail, @dose] do |f|%>
    <%= f.input :description %>
    <%= f.input :ingredient, :collection => Ingredient.all %>
    <%= f.submit %>
    <% end %>
  </div>
</div>
